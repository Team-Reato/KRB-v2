options:
	# 카린봇 Original Setting
	# a부터 z까지 단일 알파벳 = 접두사
	
	a: &f[ &eTL &f] 
	b: [ ! ]
	c: &f[ &a! &f]
	
	# botonoff(0~100) = 봇의 온오프 메세지가 전달될 채널 아이디
	# log = 로그 채널
	# karinid = 카린 디스코드 아이디
	
	botonoff0: 694153082506051614
	log: 714630863089959063
	karinid: 624274923455512577
	glog: 713968917403664454
	
	# token = 카린봇 토큰
	# name = 카린봇 이름
	
	token: %{krbtoken}%
	name: KaRin Bot v2

On script load:
	set {openserver} to 3
	login to "{@token}" with the name "{@name}"
	send "{@c} &b카린봇 &e로드 &a성공&f!" to console
	set {krbpremium::624274923455512577} to true
	set {krbpremium::451335803038203904} to true
	set {automark1} to 1
	delete {naruto}
	delete {token}
	
On skript load:
	#login to "{@token}" with the name "{@name}"
	set {krbpremium::624274923455512577} to true
	set {krbpremium::451335803038203904} to true
	set {_uptime::s} to 0
	set {_uptime::m} to 0
	set {_uptime::t} to 0
	set {_uptime::d} to 0
	set {automark1} to 0
	set {개발자게임} to "개발"
	create embed:
		set title of the embed to "ON"
		set the colour of the embed to GREEN
		set the description of embed to "카린봇이 켜졌습니다."
		set the footer of embed to footer with text "KaRin Bot Engine by IHAH"
		send last created embed to channel with id "{@botonoff0}" with "{@name}"
	
on skript unload:
	send "{@c} &b카린봇 &c언로드 &a성공&f!" to console
	create embed:
		set title of the embed to "OFF"
		set the colour of the embed to RED
		set the description of embed to "카린봇이 조금 뒤 꺼집니다."
		set the footer of embed to footer with text "KaRin Bot Engine by IHAH"
		send last created embed to channel with id "{@botonoff0}" with "{@name}"

