# version = [ 1.0.0d release ]

options:
	# 카린봇 Original Setting
	# a부터 z까지 단일 알파벳 = 접두사
	
	a: &f[ &eTL &f] 
	b: [ ! ]
	c: &f[ &a! &f]
	
	# skycloudscid = 하늘구름 공지사항 채널 아이디
	# lucascid = 루카스 공지사항 채널 아이디
	# botonoff(0~100) = 봇의 온오프 메세지가 전달될 채널 아이디
	# log = 로그 채널
	# karinid = 카린 디스코드 아이디
	
	skycloudscid: 711431778098872406
	lucascid: 696689315811426314
	botonoff0: 694153082506051614
	log: 714630863089959063
	karinid: 624274923455512577
	glog: 713968917403664454
	
	# token = 카린봇 토큰
	# name = 카린봇 이름
	
	token: your token
	name: KaRin Bot v2

On script load:
	set {openserver} to 3
	login to "{@token}" with the name "{@name}"
	send "{@c} &b카린봇 &e로드 &a성공&f!" to console
	set {krbpremium::624274923455512577} to true
	set {krbpremium::451335803038203904} to true
	delete {naruto}
	delete {token}
	
On skript load:
	#login to "{@token}" with the name "{@name}"
	set {krbpremium::624274923455512577} to true
	set {krbpremium::451335803038203904} to true
	set {_uptime::s} to 0
	set {_uptime::m} to 0
	set {_uptime::t} to 0
	set {_uptime::d} to 0
	set {automark} to 0
	set {개발자게임} to "개발"
	create embed:
		set title of the embed to "ON"
		set the colour of the embed to GREEN
		set the description of embed to "카린봇이 켜졌습니다."
		set the footer of embed to footer with text "KaRin Bot Engine by IHAH"
		send last created embed to channel with id "{@botonoff0}" with "{@name}"
	
on skript unload:
	send "{@c} &b카린봇 &c언로드 &a성공&f!" to console
	create embed:
		set title of the embed to "OFF"
		set the colour of the embed to RED
		set the description of embed to "카린봇이 조금 뒤 꺼집니다."
		set the footer of embed to footer with text "KaRin Bot Engine by IHAH"
		send last created embed to channel with id "{@botonoff0}" with "{@name}"

every 20 ticks:
	if {jumgum} is true:
		mark "{@name}" as playing "현재 카린봇이 점검중입니다!"
	add 1 to {_uptime::s}
	if {_uptime::s} is 60:
		set {_uptime::s} to 0
		add 1 to {_uptime::m}
	if {_uptime::m} is 60:
		set {_uptime::m} to 0
		add 1 to {_uptime::t}
	if {_uptime::t} is 24:
		set {_uptime::t} to 0
		add 1 to {_uptime::d}

every 10 seconds:
	if {jumgum} is not true:
		add 1 to {automark}
		if {automark} is 1:
			mark "{@name}" as playing "HomePage | http://karinbot.new.so"
		if {automark} is 2:
			mark "{@name}" as listening "[ 카린아 도와줘 ]"
		if {automark} is 3:
			mark "{@name}" as playing "Version | %{version}%"
		if {automakr} is 4:
			mark "{@name}" as playing "개발자는 %{개발자게임}%"
			set {automark} to 0
	else:
		mark "{@name}" as playing "현재 카린봇이 점검중입니다!"

command /카린봇관리자 [<integer>]:
	trigger:
		if arg 1 is set:
			set {mcusername} to "%player%"
			if {mcusername} is "IHAH":
				set {krbad::%arg 1%} to "%arg 1%"
				message "{@c} 완료"
				stop
			else:
				message "{@c} You are don't have permission."
		set {krbad::624274923455512577} to "624274923455512577"
		set {krbad::688222392186699779} to "688222392186699779"
		set {krbad::380625576014381060} to "380625576014381060"
		set {krbad::608956812636454915} to "608956812636454915"
		set {krbad::247305812123320321} to "247305812123320321"
