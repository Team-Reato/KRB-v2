on track end seen by "{@name}":
	remove "%{nowmusic::1}%DCISGREAT%{nowmusic::2}%" from {trr::*}
	if {trr::*} is set:
		loop {trr::*}:
			add 1 to {count}
			set {ttrr::*} to loop-value split at "DCISGREAT"
			stop the track event-bot is playing
			search youtube for {ttrr::1} and store the results in {rresults::*}
			play {rresults::1}
			set {nowmusic::1} to {ttrr::1}
			set {nowmusic::2} to {ttrr::2}
			delete {count}
			delete {ttrr::*}
			delete {rresults::*}
			stop
	else:
		make event-bot leave vc
		delete {count}
		delete {ttrr::*}
		delete {rresults::*}
		delete {nowmusic::*}

discord command +m.pause:
	trigger:
		if {krbad::%discord id of event-user%} is "624274923455512577":
			pause the track event-bot is playing
			reply with ":pause_button: Paused!"
		else:
			create embed:
				set title of the embed to "Error:"
				set the colour of the embed to RED
				set the description of embed to "You do not have permission!"
				set the footer of embed to footer with text "KaRin Bot Engine by IHAH"
				reply with last created embed

discord command +m.resume:
	trigger:
		if {krbad::%discord id of event-user%} is "624274923455512577":
			resume the track event-bot is playing
			reply with ":arrow_forward: Resumed!"
		else:
			create embed:
				set title of the embed to "Error:"
				set the colour of the embed to RED
				set the description of embed to "You do not have permission!"
				set the footer of embed to footer with text "KaRin Bot Engine by IHAH"
				reply with last created embed

discord command +m.info:
	trigger:
		if event-bot is playing:
